{#
########################################################
    partial: head
########################################################
#}

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {% cache if not craft.config.devMode %}
    <!-- favicons -->
    {# Mit dem Favicon Generator erstellen: http://realfavicongenerator.net/ #}

    <!-- seo / social -->
    {% hook 'seomaticRender' %}
    {% endcache %}

    <!-- stylesheet -->
    {% if craft.config.devMode %}
        <link rel="stylesheet" href="/dev/styles/main.css">
        <!--[if lt IE 10]>
        <link rel="stylesheet" href="/dev/styles/fallback.css">
        <![endif]-->
    {% else %}
        {% cache if not craft.config.devMode %}
        <link rel="stylesheet" href="{{ craft.cacheBuster.bust('/assets/styles/main.min.css') }}">
        <!--[if lt IE 10]>
        <link rel="stylesheet" href="{{ craft.cacheBuster.bust('/assets/styles/fallback.min.css') }}">
        <![endif]-->
        {% endcache %}
    {% endif %}
</head>
